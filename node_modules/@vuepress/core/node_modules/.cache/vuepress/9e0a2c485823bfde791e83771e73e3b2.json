   <!--          single-line-->\n        <!--          @input=\"searchCallback\"-->\n        <!--        />-->\n        <SearchBox />\n      </v-fade-transition>\n    </template>\n\n    <template v-else>\n      <v-fade-transition hide-on-leave>\n        <v-app-bar-title\n          id=\"app-bar-title\"\n          :style=\"titleStyle\"\n          v-text=\"$page.title || $siteTitle\"\n        />\n      </v-fade-transition>\n\n      <v-spacer />\n\n      <v-fade-transition hide-on-leave>\n        <v-btn\n          icon\n          @click=\"showSearchBar=true\"\n        >\n          <v-icon>mdi-magnify</v-icon>\n        </v-btn>\n      </v-fade-transition>\n    </template>\n\n    <!--    <template v-if=\"isAuthenticated\">-->\n    <!--      <v-avatar-->\n    <!--        size=\"36\"-->\n    <!--        contain-->\n    <!--      >-->\n    <!--        <v-img :src=\"profile.avatar_url\">-->\n    <!--          <template #placeholder>-->\n    <!--            <PicturePlaceholder size=\"36\" />-->\n    <!--          </template>-->\n    <!--        </v-img>-->\n    <!--      </v-avatar>-->\n\n    <!--      &lt;!&ndash;  移动端太挤了放不下  &ndash;&gt;-->\n    <!--      <template v-if=\"!mobile\">-->\n    <!--        <span class=\"ml-2\">-->\n    <!--          {{ profile.first_name }}-->\n    <!--        </span>-->\n    <!--      </template>-->\n    <!--    </template>-->\n\n    <v-btn\n      icon\n      @click=\"$emit('toggleSettings')\"\n    >\n      <v-icon>mdi-cog-outline</v-icon>\n    </v-btn>\n\n    <template\n      v-if=\"$currentTags\"\n      #extension\n    >\n      <TagTab />\n    </template>\n\n    <!--    <v-progress-linear-->\n    <!--      :active=\"$store.state.appbarLoading\"-->\n    <!--      indeterminate-->\n    <!--      absolute-->\n    <!--      bottom-->\n    <!--      color=\"yellow accent-4\"-->\n    <!--    />-->\n  </v-app-bar>\n</template>\n\n\n<script>\nimport SearchBox from \"@SearchBox\";\nimport TagTab from \"./extensions/TagTab\";\n\nconst appbar_background_url = \"/theme/appbar_background.png\";\n\nexport default {\n  components: {\n    TagTab,\n    SearchBox,\n  },\n\n  data() {\n    return {\n      showSearchBar: false,\n      keyword: '',\n      titleSize: 20,\n    }\n  },\n\n  computed: {\n    imageUrl() {\n      const image = this.$page.frontmatter.image;\n      return image ? image : appbar_background_url;\n    },\n    mobile() {\n      return this.$vuetify.breakpoint.xs;\n    },\n    titleStyle() {\n      return {fontSize: this.titleSize + \"px\"};\n    }\n  },\n\n  mounted() {\n    // 监听 appbar 高度改变 appbar-title 大小\n    // 这里选择 appbar.computedOpacity 作为监听对象（appbar 最小时为 0，最大时为 1）\n    this.titleSize = 20 + this.$refs.appbar.computedOpacity * 20;\n    this.$watch(\n      () => this.$refs.appbar.computedOpacity,\n      (val) => this.titleSize = 20 + val * 20\n    );\n  }\n};\n</script>"]}]}